{
  "name": "arttemplate-syntax",
  "displayName": "ArtTemplate Syntax",
  "description": "Syntax highlighting for Art Template",
  "publisher": "Phownix",
  "version": "0.1.2",
  "icon": "images/icon.png",
  "author": {
    "name": "Phownix",
    "email": "paulogutierrez211@gmail.com",
    "url": "https://github.com/Phownix/"
  },
  "homepage": "https://github.com/Phownix/ArtTemplate-Syntax",
  "repository": {
    "type": "git",
    "url": "https://github.com/Phownix/ArtTemplate-Syntax"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/art.json"
      }
    ],
    "languages": [
      {
        "id": "html",
        "aliases": [
          "Art Template",
          "art-template"
        ],
        "extensions": [ ".art" ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/icon.svg",
          "dark": "./icons/icon.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "html",
        "scopeName": "source.art",
        "path": "./syntaxes/art.tmLanguage.json",
        "embeddedLanguages": {
          "html" : "source.art"
        }
      }
    ]
  }
}
