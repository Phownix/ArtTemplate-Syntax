{
  "name": "arttemplate-syntax",
  "displayName": "ArtTemplate Syntax",
  "description": "Syntax highlighting for Art Template",
  "publisher": "Phownix",
  "version": "0.1.2",
  "icon": "images/icon.png",
  "author": {
    "name": "Phownix",
    "email": "paulogutierrez211@gmail.com",
    "url": "https://github.com/Phownix/"
  },
  "homepage": "https://github.com/Phownix/ArtTemplate-Syntax",
  "repository": {
    "type": "git",
    "url": "https://github.com/Phownix/ArtTemplate-Syntax"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "art",
        "aliases": [
          "Art Template",
          "art"
        ],
        "extensions": [
          ".art"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "art",
        "scopeName": "text.html.art",
        "path": "./syntaxes/art.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "art",
        "path": "./snippets/art.json"
      },
      {
        "language": "art",
        "path": "./snippets/html.json"
      }
    ]
  }
}
